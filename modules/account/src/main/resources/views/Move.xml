<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  https://axelor.com/xml/ns/object-views/object-views_6.0.xsd">
  
	<grid model="com.axelor.account.db.Move" title="Move" name="move-grid">
		<field name="refrence"/>
		<field name="operationDate"/>
		<field name="invoice"/>
	</grid>
	<form model="com.axelor.account.db.Move" title="Move" name="move-form">
		<panel name="movePanel" title="Overview">	
			<field name="refrence"/>
			<field name="invoice"/>
			<panel-dashlet action="action-move-show-move-lines"/>
		</panel>
		<panel sidebar="true" title="Dates">
			<field name="operationDate"/>
		</panel>
	</form>
	<action-view name="action-move-show-move-lines" title="Move Lines">
		<view type="grid" name="move-line-grid"/>
	</action-view>
  </object-views>