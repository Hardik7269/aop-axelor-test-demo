<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_6.0.xsd">

	<module package="com.axelor.contact.db" name="contact" />
	<entity name="Event">
		<string name="title" title="Title" required="true"/>
		<datetime name="startDateT" title="Start Date" required="true"/>
		<datetime name="endDateT" title="End Date" required="true"/>
		<integer name="statusSelect" title="Status" required="true" selection="contact.event.status.select"/>
		<integer name="typeSelect" title="Type" selection="contact.event.type.select"/>
		<string name="notes" title="Notes" large="true"/>
		<many-to-one name="plannedFor" ref="com.axelor.contact.db.Contact"  title="Planned for"/>
		<extra-code >
		<![CDATA[
			public static final int STATUS_DRAFT = 0;
			public static final int STATUS_PROPOSED = 1;
			public static final int TYPE_OTHER = 0;
		]]>
		</extra-code>1
		
		
	</entity>

	

</domain-models>